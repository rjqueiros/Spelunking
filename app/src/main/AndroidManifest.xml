<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kris.spelunking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Intro" />
        <activity android:name=".South" />
        <activity android:name=".TalkOrc" />
        <activity android:name=".FightOrc" />
        <activity android:name=".Intersection" />
        <activity android:name=".East" />
        <activity android:name=".West" />
        <activity android:name=".North" />
        <activity android:name=".FightGoblin"></activity>
    </application>

</manifest>